			<script type="text/javascript" src="{{asset('js/imagesloaded.pkgd.min.js')}}" defer></script>
			<script type="text/javascript" src="{{asset('js/bootstrap.min.js')}}" defer></script>			
		{% endblock %}
	</head>
	<body>
		<header>
			{% block header %}
				{% include "partials/navbar.html.twig" %}
				{% if block('carousel') is defined %}
					{% include "partials/headerHome.html.twig" %}
				{% endif %}
			{% endblock %}
		</header>
		<main>
			{% block main %}
				{% if block('sidebar') is defined %}
					{% include "partials/sidebar.html.twig" %}
				{% endif %}
			{% endblock %}
		</main>
		<footer>
			{% block footer %}
				{% include "partials/footer.html.twig" %}
			{% endblock %}
		</footer>
	</body>
</html>
